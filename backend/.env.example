# Backend Environment Variables
# Copy this file to .env and fill in your actual API keys

# Hugging Face API Token for LLM models
HUGGING_FACE_API_TOKEN=your_hugging_face_token_here

# Alpha Vantage API Key for real-time stock data (25 calls/day free)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# Finnhub API Key for fallback stock data (60 calls/min free)
FINNHUB_API_KEY=your_finnhub_key_here

# Optional: Set to production when deploying
ENVIRONMENT=development
